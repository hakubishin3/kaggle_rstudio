{
    "features": [
        "Basic"
    ],
    "cv": {
        "method": "GroupKFold"
    },
    "model": {
        "name": "lightgbm",
        "model_params": {
            "boosting_type": "gbdt",
            "objective": "regression",
            "metric": "rmse",
            "learning_rate": 0.1,
            "num_leaves": 100,
            "max_depth": 8,
            "min_child_samples": 100,
            "max_bin": 100,
            "subsample": 0.7,
            "subsample_freq": 1,
            "colsample_bytree": 0.5,
            "seed": 71,
            "bagging_seed": 71,
            "feature_fraction_seed": 71,
            "drop_seed": 71,
            "verbose": -1
        },
        "train_params": {
            "num_boost_round": 350,
            "early_stopping_rounds": 50,
            "verbose_eval": 50
        }
    },
    "dataset": {
        "input_directory": "data/input/",
        "intermediate_directory": "data/interim/",
        "feature_directory": "data/feature/",
        "output_directory": "data/output/",
        "files": {
            "train": "train.ftr",
            "test": "test.ftr"
        }
    },
    "pre_processing": {
        "down_sampling": {
            "enabled": false,
            "bagging_size": 1,
            "random_state": 71
        },
        "getdata_bounces_notnull": {
            "enabled": false
        }
    },
    "post_processing": {
        "bounces_process": {
            "enabled": true
        }
    }
}
